---
import Base from '../../layouts/base.astro';
---
<Base title="Canton | Beanfreaks" description="Beanfreaks Canton">
  <div class="grid gap-8 md:grid-cols-2 md:items-start">
    <div>
      <h1 class="text-3xl font-semibold tracking-tight">Canton</h1>
      <p class="mt-3 text-zinc-700">124 Cowbridge Road East.</p>
    </div>
    <img class="aspect-[4/3] w-full rounded-2xl object-cover" src={import.meta.env.BASE_URL + 'img/store-canton.jpg'} alt="Beanfreaks Canton" width="1200" height="900" loading="lazy" decoding="async" />
  </div>
</Base>
